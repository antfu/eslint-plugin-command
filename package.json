{
  "name": "eslint-plugin-command",
  "type": "module",
  "version": "3.4.0",
  "packageManager": "pnpm@10.25.0",
  "description": "Comment-as-command for one-off codemod with ESLint",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-plugin-command#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-plugin-command.git"
  },
  "bugs": "https://github.com/antfu/eslint-plugin-command/issues",
  "keywords": [
    "eslint-plugin",
    "codemod"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./commands": "./dist/commands.mjs",
    "./config": "./dist/config.mjs",
    "./types": "./dist/types.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "tsdown && eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "test": "vitest",
    "docs": "nr -C docs docs:dev",
    "docs:build": "nr -C docs docs:build",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "dependencies": {
    "@es-joy/jsdoccomment": "catalog:prod"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/ni": "catalog:dev",
    "@antfu/utils": "catalog:dev",
    "@eslint/config-inspector": "catalog:dev",
    "@types/lodash.merge": "catalog:dev",
    "@types/node": "catalog:dev",
    "@types/semver": "catalog:dev",
    "@typescript-eslint/rule-tester": "catalog:dev",
    "@typescript-eslint/typescript-estree": "catalog:dev",
    "@typescript-eslint/utils": "catalog:dev",
    "@vitest/ui": "catalog:dev",
    "bumpp": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-vitest-rule-tester": "catalog:dev",
    "fast-glob": "catalog:dev",
    "lint-staged": "catalog:dev",
    "lodash.merge": "catalog:dev",
    "pnpm": "catalog:dev",
    "rimraf": "catalog:dev",
    "semver": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vitest": "catalog:dev"
  },
  "resolutions": {
    "chokidar": "catalog:dev",
    "eslint-plugin-command": "workspace:*"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
